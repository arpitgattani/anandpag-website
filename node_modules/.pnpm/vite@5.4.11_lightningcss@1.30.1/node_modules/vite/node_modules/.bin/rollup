#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Research Anand PAG-V2.0/node_modules/.pnpm/@rollup+wasm-node@4.57.1/node_modules/@rollup/wasm-node/dist/bin/node_modules:/mnt/c/Research Anand PAG-V2.0/node_modules/.pnpm/@rollup+wasm-node@4.57.1/node_modules/@rollup/wasm-node/dist/node_modules:/mnt/c/Research Anand PAG-V2.0/node_modules/.pnpm/@rollup+wasm-node@4.57.1/node_modules/@rollup/wasm-node/node_modules:/mnt/c/Research Anand PAG-V2.0/node_modules/.pnpm/@rollup+wasm-node@4.57.1/node_modules/@rollup/node_modules:/mnt/c/Research Anand PAG-V2.0/node_modules/.pnpm/@rollup+wasm-node@4.57.1/node_modules:/mnt/c/Research Anand PAG-V2.0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Research Anand PAG-V2.0/node_modules/.pnpm/@rollup+wasm-node@4.57.1/node_modules/@rollup/wasm-node/dist/bin/node_modules:/mnt/c/Research Anand PAG-V2.0/node_modules/.pnpm/@rollup+wasm-node@4.57.1/node_modules/@rollup/wasm-node/dist/node_modules:/mnt/c/Research Anand PAG-V2.0/node_modules/.pnpm/@rollup+wasm-node@4.57.1/node_modules/@rollup/wasm-node/node_modules:/mnt/c/Research Anand PAG-V2.0/node_modules/.pnpm/@rollup+wasm-node@4.57.1/node_modules/@rollup/node_modules:/mnt/c/Research Anand PAG-V2.0/node_modules/.pnpm/@rollup+wasm-node@4.57.1/node_modules:/mnt/c/Research Anand PAG-V2.0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@rollup+wasm-node@4.57.1/node_modules/@rollup/wasm-node/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../@rollup+wasm-node@4.57.1/node_modules/@rollup/wasm-node/dist/bin/rollup" "$@"
fi
